{
    "cSpell.enabled": false,
    "workbench.colorCustomizations": {
        "activityBar.background": "#4D0965",
        "titleBar.activeBackground": "#6B0C8D",
        "titleBar.activeForeground": "#FDFAFF"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "googletest.failed",
                "settings": {
                    "foreground": "#f00"
                }
            },
            {
                "scope": "googletest.passed",
                "settings": {
                    "foreground": "#0f0"
                }
            },
            {
                "scope": "googletest.run",
                "settings": {
                    "foreground": "#0f0"
                }
            }
        ]
    },
    "cmake.configureArgs": [
        "-DCMAKE_TOOLCHAIN_FILE=C:\\buildtools\\vcpkg\\scripts\\buildsystems\\vcpkg.cmake"
    ],
    "gtest-adapter.debugConfig": [
        "(lldb) Launch"
    ],
    "gtest-adapter.supportLocation": false,
    "sonarlint.pathToCompileCommands": "${workspaceFolder}\\build\\compile_commands.json"
}